      "Properties": {
        "ApiId": {
          "Fn::GetAtt": [
            "GameGraphqlApi379C4840",
            "ApiId",
          ],
        },
        "Definition": "schema {
  query: Query
  mutation: Mutation
}
type Mutation {
  joinGame(id: ID!): Event
  playGame(id: ID! name: String! data: EventDataInput): Event
  createGame(email: AWSEmail!): Game
}
type Query {
  getEventById(id: ID!): Event
  listEventsByGameId(gameId: ID!): [Event]
  getGameById(id: ID!): Game
  listGamesByOpponentId: [Game]
  listGames: [Game]
}
type Event {
  created: AWSDateTime!
  data: EventData
  gameId: ID!
  id: ID!
  name: ID!
  opponentId: ID!
  updated: AWSDateTime
  userId: ID!
}
type EventData {
  card: String
  from: String!
  fromPosition: Int
  to: String!
  toPosition: Int
}
input EventDataInput {
  card: String
  from: String!
  fromPosition: Int
  to: String!
  toPosition: Int
}
type Game {
  created: AWSDateTime!
  id: ID!
  userEmail: ID!
  userId: ID!
  opponentEmail: ID!
  opponentId: ID!
  updated: AWSDateTime
}
",
      },
      "Type": "AWS::AppSync::GraphQLSchema",
    },
